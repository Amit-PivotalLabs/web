<div class="md-main__inner md-grid" data-md-component="container">
  <div class="md-sidebar md-sidebar--primary" data-md-component="navigation">
    <div class="md-sidebar_scrollwrap">
      <div class="md-sidebar__inner">
        <nav class="md-nav md-nav--primary" data-md-level="0">
          <ul class="md-nav__list" data-md-scrollfix>
            <li class="md-nav__item md-nav__item--active md-nav__item--nested">
              <nav class="md-nav" data-md-component="collapsible" data-md-level="1">
                <label class="md-nav__title">Releases</label>
                <ul class="md-nav__list" data-md-scrollfix>
                  <li class="md-nav__item">
                    <a href="/releases/" title="Releases" class="md-nav__link">&laquo; All Releases</a>
                  </li>
                  <li class="md-nav__item">
                    <a href="/releases/" title="cloudfoundry" class="md-nav__link">&laquo; cloudfoundry</a>
                  </li>
                  <li class="md-nav__item">
                    <a href="/releases/" title="bosh" class="md-nav__link">&laquo; bosh</a>
                  </li>
                  <li class="md-nav__item">
                    <a href="/releases/" title="v{{ .Release.Version }}" class="md-nav__link">&laquo; v{{ .Release.Version }}</a>
                  </li>
                  <li class="md-nav__item">
                    <a href="{{ .Release.URL }}" title="Overview" class="md-nav__link">Overview</a>
                  </li>
                  {{ if .Release.Jobs }}
                    <li class="md-nav__item md-nav__item--nested">
                      <input class="md-toggle md-nav__toggle" data-md-toggle="nav-4-11" type="checkbox" id="nav-4-11" checked="">
                      <label class="md-nav__link" for="nav-4-11">
                        Jobs
                      </label>
                      <nav class="md-nav" data-md-component="collapsible" data-md-level="2" style="display: block; overflow: visible;">
                        <label class="md-nav__title" for="nav-4-11">
                          Jobs
                        </label>
                        <ul class="md-nav__list" data-md-scrollfix="">
                          {{ range .Release.Jobs }}
                            <li class="md-nav__item">
                              <input class="md-toggle md-nav__toggle" data-md-toggle="toc" type="checkbox" id="toc">
                              <label class="md-nav__link" for="toc">
                                {{ .Name }}
                              </label>
                              <a href="{{ .URL }}" title="{{ .Name }}" class="md-nav__link">
                                {{ .Name }}
                              </a>
                            </li>
                          {{ end }}
                        </ul>
                      </nav>
                    </li>
                  {{ end }}
                  {{ if .Release.Packages }}
                    <li class="md-nav__item md-nav__item--nested">
                      <input class="md-toggle md-nav__toggle" data-md-toggle="nav-4-11" type="checkbox" id="nav-4-11" checked="">
                      <label class="md-nav__link" for="nav-4-11">
                        Packages
                      </label>
                      <nav class="md-nav" data-md-component="collapsible" data-md-level="2" style="display: block; overflow: visible;">
                        <label class="md-nav__title" for="nav-4-11">
                          Packages
                        </label>
                        <ul class="md-nav__list" data-md-scrollfix="">
                          {{ range .Release.Packages }}
                            <li class="md-nav__item">
                              <input class="md-toggle md-nav__toggle" data-md-toggle="toc" type="checkbox" id="toc">
                              <label class="md-nav__link" for="toc">
                                {{ .Name }}
                              </label>
                              <a href="{{ .URL }}" title="{{ .Name }}" class="md-nav__link">
                                {{ .Name }}
                              </a>
                            </li>
                          {{ end }}
                        </ul>
                      </nav>
                    </li>
                  {{ end }}
                </ul>
              </nav>
            </li>
          </ul>
        </nav>
      </div>
    </div>
  </div>
  <div class="md-sidebar md-sidebar--secondary" data-md-component="toc">
    <div class="md-sidebar__scrollwrap">
      <div class="md-sidebar__inner">
        <nav class="md-nav md-nav--secondary">
          <label class="md-nav__title" for="toc">Table of contents</label>
          <ul class="md-nav__list" data-md-scrollfix="">
            <li class="md-nav__item">
              <a href="#overview" title="Overview" class="md-nav__link">Overview</a>
            </li>
            <li class="md-nav__item">
              <a href="#properties" title="Properties" class="md-nav__link">Properties</a>
            </li>
            {{ if .Templates }}
              <li class="md-nav__item">
                <a href="#templates" title="Templates" class="md-nav__link">Templates</a>
              </li>
            {{ end }}
            {{ if .Packages }}
              <li class="md-nav__item">
                <a href="#packages" title="Packages" class="md-nav__link">Packages</a>
              </li>
            {{ end }}
          </ul>
        </nav>
      </div>
    </div>
  </div>
  <div class="md-content">
    <article class="md-content__inner md-typeset">
      <div class="bosh-schema">
        <h1>{{ .Name }} job <br><small>from {{ .Release.Name }}/{{ .Release.Version }}</small></h1>

        {{ if .Description }}
          <p>{{ .Description }}</p>
        {{ end }}

        {{ if .HasGithubURL }}
          <p>
            Github source:
            <a href="{{ .GithubURL }}"><code>{{ .Release.CommitHash }}</code></a> or
            <a href="{{ .GithubURLOnMaster }}">master</a> branch
          </p>
        {{ end }}

        <h2 id="properties">Properties<a class="headerlink" href="#properties" title="Permanent link">&para;</a></h2>

        {{ template "jobs/_properties" . }}

        {{ template "jobs/_templates_and_pkgs" . }}
      </article>
    </article>
  </div>
</main>
